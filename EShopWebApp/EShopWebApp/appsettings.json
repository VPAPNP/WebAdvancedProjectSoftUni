{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {

    "DefaultConnection": "Server=tcp:$(SQL_DATABASE_NAME),1433;Initial Catalog=$(SQL_SERVER_NAME);Persist Security Info=False;User ID=$(SQL_USER);Password=$(SQL_PASSWORD);MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"


  }
  





}
