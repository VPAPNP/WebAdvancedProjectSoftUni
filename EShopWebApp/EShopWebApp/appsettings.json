{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=tcp:$(SQL_SERVER_NAME).database.windows.net,1433;Initial Catalog=$(SQL_DATABASE_NAME);Persist Security Info=False;User ID=$(SQL_USER);Password=$(SQL_PASSWORD);MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"


  },
  "GoogleKeys": {
    "ClientId": "${{secret.GOOGLE_CLIENT_ID}}",
    "ClientSecret": "${{secret.GOOGLE_API_KEY}}"
  }
  





}
